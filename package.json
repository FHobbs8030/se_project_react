{
  "name": "se_project_react",
  "private": true,
  "version": "0.0.0",
  "homepage": "https://fhobbs8030.github.io/se_project_react",
  "scripts": {
    "dev": "vite --open --port 5175",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives",
    "lint:fix": "eslint . --ext js,jsx --fix",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "server": "json-server --watch db.json --id _id --port 3001",
    "mock": "json-server --watch db.json --port 3004 --static public",
    "dev:all": "concurrently -k -n APP,API -c blue,magenta \"vite --port 5175\" \"json-server --watch db.json --port 3004 --static public\""
  },
  "dependencies": {
    "celebrate": "^15.0.3",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.1.0",
    "concurrently": "^9.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "gh-pages": "^6.0.0",
    "json-server": "^1.0.0-beta.3",
    "vite": "^6.3.5"
  }
}
